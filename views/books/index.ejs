<h2>Search books</h2>
<form action="/books" method="GET">
  <div>
    <label>Title</label>
    <input type="text" name="title" value="<%= searchOptions.title %> " />
  </div>
  <div>
    <label>Publish Before</label>
    <input
      type="text"
      name="publishedBefore"
      value="<%= searchOptions.publishedBefore %> "
    />
  </div>
  <div>
    <label>Publish After</label>
    <input
      type="text"
      name="publishedAfter"
      value="<%= searchOptions.publishedAfter %> "
    />
  </div>
  <button type="submit">Search</button>
</form>
<br />

<div>
  <% books.forEach(book=>{ %>
  <img src="<%= book.coverImagePath %> " alt="" height="150" width="100" />
  <% }) %>
</div>
